{
    "name": "proklung/phpunit-testing-tools",
    "description": "PHPUNIT testing tools.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/phpunit.testing.tools/issues",
        "source": "https://github.com/ProklUng/phpunit.testing.tools"
    },
    "autoload": {
        "psr-4": {
            "Prokl\\TestingTools\\": "src"
        },
        "files": [
            "src/Functions/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Prokl\\TestingTools\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ProklUng/console-test"
    }],
    "require": {
        "phpunit/phpunit": "^7.5.2 || ^8.0 || ^9.0",
        "mockery/mockery": "^1.4",
        "fzaninotto/faker": "^1.9.2",
        "illuminate/support": "~6 | ~7 | ~8",
        "illuminate/container": "~6 | ~7 | ~8",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "php-mock/php-mock-mockery": "^1.3",
        "helmich/phpunit-json-assert": "^3.4",
        "estahn/phpunit-json-assertions": "^v4.0.0",
        "symfony/routing": "^4.4 || ^5.0",
        "dms/phpunit-arraysubset-asserts": "^0.3.0",
        "symfony/dom-crawler": "^4.4 || ^5.0",
        "symfony/yaml": "^4.4 || ^5.0",
        "symfony/property-info": "^4.4 || ^5.0",
        "phpstan/phpstan": "^0.12.64",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-mockery": "^0.12.11",
        "phpstan/phpstan-beberlei-assert": "^0.12.4",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpstan/phpstan-symfony": "^0.12.12",
        "phpstan/phpstan-doctrine": "^0.12.26",
        "vimeo/psalm": "^4.8.1",
        "psalm/plugin-symfony": "^2.4",
        "psalm/plugin-mockery": "^0.9.0",
        "dg/bypass-finals": "^1.3",
        "squizlabs/php_codesniffer": "^3.6.0",
        "brain/monkey": "^2.6.0",
        "friendsofphp/proxy-manager-lts": "^1.0",
        "ext-json": "*",
        "zenstruck/console-test": "^1.0"
    },
    "require-dev": {
        "icanhazstring/composer-unused": "^0.7.5"
    }
}
